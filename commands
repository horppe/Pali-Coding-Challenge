# On Linux: Curl post request
    curl --header "Content-Type: application/json" \
  --request POST \
  --data '{"mealIdList": ["52795", "52956", "52831", "52854"]}' \
  http://localhost:3000/


  # On Windows: Curl post request
    curl --header "Content-Type: application/json"  --request POST   --data '{"""username""":"""xyz""","""password""":"""xyz"""}'  http://localhost:3000/api/login